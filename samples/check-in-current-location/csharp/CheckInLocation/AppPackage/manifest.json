{
  "$schema": "https://developer.microsoft.com/en-us/json-schemas/teams/v1.10/MicrosoftTeams.schema.json",
  "manifestVersion": "1.10",
  "version": "1.0.0",
  "id": "{{Microsoft-App-Id}}",
  "packageName": "com.microsoft.teams.checkinlocation",
  "developer": {
    "name": "Microsoft",
    "websiteUrl": "https://www.microsoft.com",
    "privacyUrl": "https://www.microsoft.com/privacy",
    "termsOfUseUrl": "https://www.microsoft.com/termsofuse"
  },
  "name": {
    "short": "Check in location",
    "full": "Get check in location of user"
  },
  "description": {
    "short": "Check in location",
    "full": "This sample demos to get current location and time of user."
  },
  "icons": {
    "outline": "outline.png",
    "color": "color.png"
  },
  "accentColor": "#60A18E",
  "bots": [
    {
      "botId": "{{Microsoft-App-Id}}",
      "scopes": [
        "personal"
      ],
      "isNotificationOnly": false
    }
  ],
  "permissions": [ "identity", "messageTeamMembers" ],
  "devicePermissions": [
    "geolocation"
  ],
  "validDomains": [
    "{{domain-name}}",
    "token.botframework.com"
  ]
}
