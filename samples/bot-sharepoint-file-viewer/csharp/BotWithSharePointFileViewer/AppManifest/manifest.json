{
  "$schema": "https://developer.microsoft.com/en-us/json-schemas/teams/v1.11/MicrosoftTeams.schema.json",
  "manifestVersion": "1.11",
  "version": "1.0.0",
  "id": "<<Microsoft-App-Id>>",
  "packageName": "com.microsoft.teams.botsharepointfileviewer",
  "developer": {
    "name": "Microsoft",
    "websiteUrl": "https://www.microsoft.com",
    "privacyUrl": "https://www.microsoft.com/privacy",
    "termsOfUseUrl": "https://www.microsoft.com/termsofuse"
  },
  "name": {
    "short": "Bot sharepoint file viewer",
    "full": "Bot sharepoint file viewer"
  },
  "description": {
    "short": "Bot sharepoint file viewer",
    "full": "This sample demos a bot which shares SharePoint file which can be viewed in Teams file viewer"
  },
  "icons": {
    "outline": "outline.png",
    "color": "color.png"
  },
  "bots": [
    {
      "botId": "<<Microsoft-App-Id>>",
      "scopes": [
        "team",
        "personal",
        "groupchat"
      ],
      "isNotificationOnly": false,
      "supportsFiles": true
    }
  ],
  "accentColor": "#60A18E",
  "permissions": [ "identity", "messageTeamMembers" ],
  "validDomains": [
    "<<Domain-name>>",
    "token.botframework.com"
  ],
  "webApplicationInfo": {
    "id": "<<Microsoft-App-Id>>",
    "resource": "api://botid-<<Microsoft-App-Id>>"
  }
}