
{
  "$schema": "https://developer.microsoft.com/en-us/json-schemas/teams/v1.7/MicrosoftTeams.schema.json",
  "manifestVersion": "1.7",
  "version": "1.0.1",
  "id": "<<CONNECTOR ID>>",
  "packageName": "com.microsoft.teams.samples.sampleapp",
  "developer": {
    "name": "Microsoft",
    "websiteUrl": "https://www.microsoft.com",
    "privacyUrl": "https://www.microsoft.com/privacy",
    "termsOfUseUrl": "https://www.microsoft.com/termsofuse"
  },
  "name": {
    "short": "Connector APP",
    "full": "Connector APP"
  },
  "description": {
    "short": "This is a small sample app we made for you, in nodejs",
    "full": "This is a small sample app we made for you, in nodejs This app has connector with authentication added for configuration page."
  },
  "icons": {
    "outline": "outline.png",
    "color": "color.png"
  },
  "accentColor": "#3F487F",
  "connectors": [
    {
      "connectorId": "<<CONNECTOR ID>>",
      "configurationUrl": "<<CONFIGURATION URL>>",
      "scopes": [
        "team"
      ]
    }
  ],
  "permissions": [
    "identity",
    "messageTeamMembers"
  ],
  "webApplicationInfo": {
    "id": "<<APP ID>>",
    "resource": "<<APP ID URI>>"
    }
}