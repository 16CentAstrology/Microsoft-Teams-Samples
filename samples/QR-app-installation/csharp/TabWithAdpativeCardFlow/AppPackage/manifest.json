{
  "$schema": "https://developer.microsoft.com/en-us/json-schemas/teams/v1.10/MicrosoftTeams.schema.json",
  "manifestVersion": "1.10",
  "version": "1.0.2",
  "id": "",
  "packageName": "com.microsoft.teams.qrappinstallation",
  "developer": {
    "name": "Microsoft",
    "websiteUrl": "https://www.microsoft.com",
    "privacyUrl": "https://www.microsoft.com/privacy",
    "termsOfUseUrl": "https://www.microsoft.com/termsofuse"
  },
  "name": {
    "short": "QRAppInstallation",
    "full": "Install app by QR"
  },
  "description": {
    "short": "Install app by QR",
    "full": "This sample demos app installation using QR code with team id"
  },
  "icons": {
    "outline": "outline.png",
    "color": "color.png"
  },
  "accentColor": "#60A18E",
  "bots": [
    {
      "botId": "",
      "scopes": [
        "team",
        "personal",
        "groupchat"
      ],
      "isNotificationOnly": false
    }
  ],
  "webApplicationInfo": {
    "id": "",
    "resource": "api://botid-/access_as_user"
  },
  "permissions": [ "identity", "messageTeamMembers" ],
  "devicePermissions": [
    "media"
  ],
  "validDomains": [
    "*.ngrok.io",
    "token.botframework.com"
  ]
}
