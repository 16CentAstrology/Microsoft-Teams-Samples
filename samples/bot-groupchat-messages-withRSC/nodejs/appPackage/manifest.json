{
    "$schema": "https://developer.microsoft.com/en-us/json-schemas/teams/v1.10/MicrosoftTeams.schema.json",
    "manifestVersion": "1.10",
    "version": "1.0.0",
    "id": "{MicrosoftAppId}",
    "packageName": "com.microsoft.teams.botgroupchatrsc",
    "developer": {
      "name": "Microsoft",
      "websiteUrl": "https://www.microsoft.com",
      "privacyUrl": "https://www.microsoft.com/privacy",
      "termsOfUseUrl": "https://www.microsoft.com/termsofuse"
    },
    "name": {
      "short": "RSC Groupchat Sample",
      "full": "Groupchat Messages with RSC"
    },
    "description": {
      "short": "Receive groupchat messages with RSC",
      "full": "This sample demos a bot with RSC permission"
    },
    "icons": {
      "outline": "outline.png",
      "color": "color.png"
    },
    "bots": [
      {
        "botId": "{MicrosoftAppId}",
        "scopes": [
          "team",
          "personal",
          "groupchat"
        ],
        "isNotificationOnly": false,
        "supportsFiles": true
      }
    ],
    "accentColor": "#60A18E",
    "permissions": [ "identity", "messageTeamMembers" ],
    "validDomains": [
      "<<DOMAIN-NAME>>",
      "token.botframework.com"
    ],
    "webApplicationInfo": {
      "id": "{MicrosoftAppId}",
      "resource": "api://botid-{MicrosoftAppId}",
      "applicationPermissions": [
        "ChatMessage.Read.Chat"
      ]
    }
  }